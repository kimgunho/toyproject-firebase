{
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "plugins": ["import", "node", "promise", "standard"],
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "no-console": "off",
      "react/prop-types": 0,
      "react/react-in-jsx-scope":0
    }
  }